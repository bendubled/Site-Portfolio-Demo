        <title>{% block title %}Test{% endblock %}</title>

		{% stylesheets output='css/final.css'
                    'libraries/bootstrap/css/bootstrap.css'
		    'libraries/font-awesome/css/font-awesome.css'
		    'libraries/animate.css'
		    'libraries/Hover/css/hover.css'
		    'css/main.css' 
                    'https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css'
                    'libraries/aos-master/dist/aos.css'
                    
                %}
		    <link type="text/css" rel="stylesheet" href="{{ asset_url }}" />
		{% endstylesheets %}
                
                {% javascripts
